{
    "problemMatcher": [
      {
        "owner": "flake8",
        "severity": "warning",
        "pattern": [
          {
            "regexp": "^(.+?):([0-9]+?):([0-9]+?): ([A-Z][0-9]+) (.+)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "message": 5,
            "code": 4
          }
        ]
      }
    ]
  }
  